{
	"name": "rtcamp/oneaccess",
	"description": "OneAccess",
	"type": "wordpress-plugin",
	"autoload": {
		"classmap": [
			"inc/"
		],
		"files": [
			"inc/helpers/custom-functions.php",
			"vendor/woocommerce/action-scheduler/action-scheduler.php"
		]
	},
	"authors": [
		{
			"name": "Utsav Patel",
			"email": "up1512001@gmail.com"
		},
		{
			"name": "rtCamp",
			"email": "marketing@rtcamp.com"
		}
	],
	"require-dev": {
		"wp-coding-standards/wpcs": "^2.3",
		"phpcompatibility/phpcompatibility-wp": "^2.1",
		"automattic/vipwpcs": "^2.3",
		"phpunit/phpunit": "^9.6",
		"wp-phpunit/wp-phpunit": "^6.2",
		"yoast/phpunit-polyfills": "^1.0"
	},
	"require": {
		"woocommerce/action-scheduler":"3.9.3"
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	},
	"scripts": {
		"format": "phpcbf --standard=phpcs.xml.dist --report-summary --report-source",
		"lint": "phpcs --standard=phpcs.xml.dist",
		"pot": "wp i18n make-pot . languages/oneaccess.pot --exclude=node_modules,vendor,tests --ignore-domain=default"
	}
}
